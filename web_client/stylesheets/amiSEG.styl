.visualizer 

    height 80vh
    display -webkit-box
    display -ms-flexbox
    display flex
    -ms-flex-wrap wrap
    flex-wrap wrap
    background-color #ffffff

.visualizerRender
    height 100%
.rendererFalse
    background-color #000
    width 0%
    height 0%
    -webkit-box-sizing border-box
    box-sizing border-box
    border 1px solid rgba(0, 0, 0, 0.2)
    border-bottom none
    position relative
    overflow hidden
    display none
    
.renderer 
    padding 0px
    background-color #fff
    width 50%
    height 488px
    -webkit-box-sizing border-box
    box-sizing border-box
    border 1px solid rgba(0, 0, 0, 0.2)
    border-bottom none
    position relative
    overflow hidden

#editor
    pointer-events none
    width 100%
    height 488px
    -webkit-box-sizing border-box
    box-sizing border-box
    border-bottom none
    position absolute
    overflow hidden

.domExploring
    padding 0px
    pointer-events none
    height 488px
    -webkit-box-sizing border-box
    box-sizing border-box
    border 1px solid rgba(0, 0, 0, 0.0)
    border-bottom none
    overflow hidden

.domEditing 
    padding 0px
    pointer-events all
    height 488px
    -webkit-box-sizing border-box
    box-sizing border-box
    border 1px solid rgba(0, 0, 0, 0.0)
    border-bottom none
    overflow hidden

#r0 
    border-top 2px solid #03a9f4
    
#tableStat
    position absolute
    height 54%
    width 50%
    overflow auto
    background-color white
    
#r1 
    border-top 2px solid #f44336
    

#r2 
    border-top 2px solid #ffeb3b


#r3 
    border-top 2px solid #8bc34a

#my-gui-container 
    right 0px
    z-index 1
    height 100%
#stats 
  position absolute
  top 0px
  height 34px
.dg.main
    width 100%
    overflow-y: auto;
    height: 80vh;
.dg.main .close-button.close-bottom
  position relative

